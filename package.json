{
  "name": "@invoicebox/minapp-sdk",
  "type": "module",
  "license": "ISC",
  "version": "3.0.0",
  "scripts": {
    "clean": "./clean.sh",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "description": "Package to communicate with Invoicebox payment page",
  "repository": {
    "type": "git",
    "url": "git@github.com:InvoiceBox/invoicebox-minapp-sdk.git"
  },
  "engines": {
    "node": "^16.17.1"
  },
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "files": [
    "**/package.json",
    "**/index.js",
    "**/*.d.ts"
  ],
  "types": "./cjs/index.d.ts",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "typescript": "^5.1.6"
  }
}
